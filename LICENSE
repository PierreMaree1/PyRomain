resultat = input("donner un nombre entre 1 et 10")
resultat = int(resultat)

if resultat == 1:
    print("I")
elif resultat == 2:
    print("II")
elif resultat == 3:
    print("III")
elif resultat == 4:
    print("IV")
elif resultat == 5:
    print("V")
elif resultat == 6:
    print("VI")
elif resultat == 7:
    print("VII")
elif resultat == 8:
    print("VIII")
elif resultat == 9:
    print("IX")
elif resultat == 10:
    print("X")
else:
    print("le chiffre doit Ãªtre entre 1 et 10")


